  <target name="compile.module.root.production" depends="register.custom.compilers" description="Compile module root; production classes">
    <mkdir dir="${root.output.dir}"/>
    <javac2 destdir="${root.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.root}/javac">
