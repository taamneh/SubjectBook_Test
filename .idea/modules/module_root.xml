      <patternset refid="excluded.from.compilation.root"/>
    </javac2>
    
    <copy todir="${root.output.dir}">
      <fileset dir="${module.root.basedir}/../../app">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.root.basedir}/../../target/scala-2.10/src_managed/main">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
